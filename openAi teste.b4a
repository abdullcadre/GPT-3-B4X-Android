	Private Panel1Lottie As Panel
	Private Label1 As Label
End Sub

Sub Activity_Create(FirstTime As Boolean)
	Activity.LoadLayout("splash")
	Label1.SetVisibleAnimated(600,True)
	aniation
	Sleep(5890)
	StartActivity(ChatUI)
	Activity.Finish
End Sub


